class Solution {
public:
    int sumDivisibleByK(vector<int>& nums, int k) {
        int n = nums.size();
        unordered_map<int, int> mp;
        for(int i = 0; i<n; i++){
            mp[nums[i]]++;
        }
        // 1->1
        // 2->2
        // 3->4
        // 4->1  
        int sum = 0;
        for(auto &it: mp){
            if(it.second %k==0){
                sum += it.first*it.second;
            }
        }
        return sum;
    }
};
